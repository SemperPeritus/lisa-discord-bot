{
  "name": "lisa-discord-bot",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-bot": "node dist/index.js",
    "start-bot-bridge": "node dist/bridge.js",
    "start-bot-cluster": "node dist/cluster.js",
    "start-bot-dev": "ts-node dist/index.js",
    "migrate": "sequelize-cli db:migrate --config src/configs/sequelize.ts --migrations-path src/migrations",
    "dev": "run-s build migrate start-bot",
    "start": "run-s migrate start-bot",
    "start-bridge": "run-s migrate start-bot-bridge",
    "start-cluster": "run-s migrate start-bot-cluster",
    "mig-gen": "npx sequelize-cli migration:generate --migrations-path src/migrations --name"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SemperPeritus/lisa-discord-bot.git"
  },
  "bugs": {
    "url": "https://github.com/SemperPeritus/lisa-discord-bot/issues"
  },
  "homepage": "https://github.com/SemperPeritus/lisa-discord-bot#readme",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "discord-cross-hosting": "^2.1.8",
    "discord-hybrid-sharding": "^1.4.3",
    "discord.js": "^13.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "i18next": "^20.4.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.1",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sequelize-cli": "^6.2.0"
  }
}
